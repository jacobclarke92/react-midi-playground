{
  "name": "midi-controller",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run watch & node server.js",
    "test": "echo \"Error: no test specified\" && exit 1",

    "watch": "npm run watch:js & npm run watch:less",
    "watch:js": "webpack --watch",
    "watch:less": "watch \"npm run build:less\" \"styles\"",

    "build": "npm run build:js & npm run build:less",
    "build:js": "watch-less -d styles -r dist -e .css -c -o",
    "build:less": "lessc styles/styles.less > dist/styles.css",

    "deploy": "npm run deploy:js & npm run deploy:less",
    "deploy:js": "webpack -p -v",
    "deploy:less": "lessc --clean-css styles/styles.less > dist/styles.css"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "express": "^4.13.3",
    "less": "^2.5.3",
    "watch": "^0.17.1",
    "webpack": "^1.12.9"
  },
  "dependencies": {
    "autobind-decorator": "^1.3.2",
    "beats": "0.0.0",
    "classnames": "^2.2.1",
    "immutable": "^3.7.6",
    "keycode": "^2.1.0",
    "lodash": "^3.10.1",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.6",
    "redux": "^3.0.5",
    "redux-localstorage": "^0.4.0",
    "web-midi-api": "^1.4.4"
  }
}
